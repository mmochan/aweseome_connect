<%- model_class = AppdotnetSocial -%>
<div class="page-header">
  <h1><%=t '.title', :default => model_class.model_name.human.pluralize %></h1>
</div>
<table class="table table-striped">
  <thead>
    <tr>
      <th><%= model_class.human_attribute_name(:id) %></th>
      <th><%= model_class.human_attribute_name(:user) %></th>
      <th><%= model_class.human_attribute_name(:email) %></th>
      <th><%= model_class.human_attribute_name(:password) %></th>
      <th><%= model_class.human_attribute_name(:created_at) %></th>
      <th><%=t '.actions', :default => t("helpers.actions") %></th>
    </tr>
  </thead>
  <tbody>
    <% @appdotnet_socials.each do |appdotnet_social| %>
      <tr>
        <td><%= link_to appdotnet_social.id, appdotnet_social_path(appdotnet_social) %></td>
        <td><%= appdotnet_social.user %></td>
        <td><%= appdotnet_social.email %></td>
        <td><%= appdotnet_social.password %></td>
        <td><%=l appdotnet_social.created_at %></td>
        <td>
          <%= link_to t('.edit', :default => t("helpers.links.edit")),
                      edit_appdotnet_social_path(appdotnet_social), :class => 'btn btn-mini' %>
          <%= link_to t('.destroy', :default => t("helpers.links.destroy")),
                      appdotnet_social_path(appdotnet_social),
                      :method => :delete,
                      :data => { :confirm => t('.confirm', :default => t("helpers.links.confirm", :default => 'Are you sure?')) },
                      :class => 'btn btn-mini btn-danger' %>
        </td>
      </tr>
    <% end %>
  </tbody>
</table>

<%= link_to t('.new', :default => t("helpers.links.new")),
            new_appdotnet_social_path,
            :class => 'btn btn-primary' %>
